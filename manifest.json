{
    "name": "HCMUS_moodle_darkmode",
    "description": "Create a Dark theme for fit@hcmus's moodle",
    "author": "Pham Phuoc Sang, Nguyen Huy Hoan",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_title": "HCMUS_moodle_change_color"
    },
    "content_scripts": [
        {
            "matches": [
                "https://courses.fit.hcmus.edu.vn/*",
                "https://courses.fit.hcmus.edu.vn/?*",
                "https://courses.ctda.hcmus.edu.vn/*",
                "https://courses.ctda.hcmus.edu.vn/?*"
                
            ],
            "js": [
                "src/jquery.js",
                "src/Main.js",
                "src/Main2.js",
                "src/Main3.js"
            ]
        }
    ]
}